<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="com.stu.view.controller.AddCourseController" prefHeight="300.0" prefWidth="400.0">
    <children>
        <Label layoutX="50.0" layoutY="30.0" text="Course Name:" />
        <TextField fx:id="courseNameField" layoutX="150.0" layoutY="26.0" />
        <Label layoutX="50.0" layoutY="70.0" text="Teacher:" />
        <TextField fx:id="teacherField" layoutX="150.0" layoutY="66.0" />
        <Label layoutX="50.0" layoutY="110.0" text="Credit:" />
        <TextField fx:id="creditField" layoutX="150.0" layoutY="106.0" />
        <Label layoutX="50.0" layoutY="150.0" text="Term:" />
        <TextField fx:id="termField" layoutX="150.0" layoutY="146.0" />
        <Button layoutX="160.0" layoutY="200.0" onAction="#addCourseButtonAction" text="Add Course" />
    </children>
</AnchorPane>